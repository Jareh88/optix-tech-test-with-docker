    "@reduxjs/toolkit": "^1.9.5",
    "dotenv": "^16.4.5",
    "polished": "^4.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.53.1",
    "react-router": "^6.11.1",
    "react-router-dom": "^6.11.1",
    "react-use": "^17.4.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.2",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
