    "globals": "^15.12.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "typescript": "^4.6.4",
    "typescript-eslint": "^8.13.0",
    "vite": "^3.1.0"
